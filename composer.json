{
    "name": "fila-virtual",
    "description": "sistema de fila virtual para uso de salas de reuni√µes",
    "type": "project",
    "homepage": "https://github.com/zendframework/zend-expressive-skeleton",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dieguinho Henrique Rodrigues",
            "homepage": "https://dhr.com/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": "^5.5 || ^7.0"        
    },
    "require-dev": {
        "phpunit/phpunit": " 4.8.9",
        "squizlabs/php_codesniffer": "^2.3"        
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8090 -t public/ public/index.php",
        "test": "phpunit"
    }
}
